{
  "name": "anl/postman-collection-generator",
  "description": "Postman collection generator",
  "keywords": [
    "postman",
    "postman collection generator",
    "postman generator",
    "api",
    "documentation"
  ],
  "license": "MIT",
  "type": "project",
  "require": {
    "php": ">=7.2",
    "symfony/filesystem": "^4.2",
    "symfony/yaml": "^4.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.4",
    "mockery/mockery": "^1.2",
    "laravel/lumen-framework": "^5.5",
    "illuminate/routing": "^5.5",
    "vlucas/phpdotenv": "^3.3"
  },
  "conflict" : {
    "symfony/yaml": "<5.0"
  },
  "autoload": {
    "psr-4": {
      "PostmanGenerator\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\PostmanGenerator\\": "tests/"
    }
  },
  "suggest": {
    "laravel/lumen-framework": "^5.5",
    "vlucas/phpdotenv": "^3.3"
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "optimize-autoloader": true
  }
}

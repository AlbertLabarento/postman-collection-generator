{
    "name": "anl/postman-collection-generator",
    "description": "Postman collection generator",
    "keywords": [
      "postman",
      "postman collection"
    ],
    "license": "MIT",
    "type": "project",
    "require": {
      "php": ">=7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.4",
        "mockery/mockery": "^1.2"
    },
    "autoload": {
      "psr-4": {
        "App\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
        "Tests\\App\\": "tests/"
      }
    },
    "minimum-stability": "dev",
    "config": {
      "optimize-autoloader": true
    }
}
